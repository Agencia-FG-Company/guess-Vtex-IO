  "rich-text#pdp__methodsPayments-card-title": {
    "props": {
      "text": "Pagamento com cartao de crédito",
      "blockClass": ["pdp__methodsPayments-card--title"]
    }
  },
  "product-installments-list#pdp__methodsPayments": {
    "props": {
      "blockClass": "pdp__methodsPayments"
    },
    "children": ["product-installments-list-item#pdp__methodsPayments"]
  },
  "product-installments-list-item#pdp__methodsPayments": {
    "props": {
      "message": "{installmentsNumber}x de R${installmentValue} sem juros"
    }
