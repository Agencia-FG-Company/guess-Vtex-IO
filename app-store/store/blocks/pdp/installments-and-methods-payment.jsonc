    "props": {
      "text": "Formas de pagamento",
      "blockClass": ["pdp__methodsPayments", "pdp__trigger"]
    }
  },
  "modal-header#pdp__methodsPayments": {
    "props": {
      "blockClass": ["pdp__methodsPayments", "header__modal"],
