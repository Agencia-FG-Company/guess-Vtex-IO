    },

    "flex-layout.col#lp__banner-duplo-left": {
        "title": "Banner duplo",
        "props": {
            "blockClass": "banner-text"
        },
        "children": [
            "image#lp__left-banner-duplo",
            "rich-text#lp__banner-duplo-left"
        ]
    },
    "rich-text#lp__banner-duplo-left": {
        "title": "texto do banner",
        "props": {
            "text": "O melhor da<br>moda masculina<br>em *sale*\n[Shop Now](/sale/masculino)",
