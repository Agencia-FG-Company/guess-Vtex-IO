{
    "store.custom#LPnew": {
        "children": [
            "flex-layout.row#lp__banner-topo",
            "responsive-layout.desktop#lp-new-desktop",
            "responsive-layout.mobile#lp-new-mobile"
        ]
    },

    // Desktop
    "responsive-layout.desktop#lp-new-desktop": {
        "children": [
            "flex-layout.row#lp__banner-duplo-v2",
            "flex-layout.row#lp__banner-trio-v2",
            "flex-layout.row#lp__banner-final",
            "flex-layout.row#seo__content"
        ]
    },
    // MOBILE
    "responsive-layout.mobile#lp-new-mobile": {
        "children": [
            "flex-layout.row#lp__banner-duplo-v2",
            "flex-layout.row#lp__banner-duplo-v2-mob",
            "flex-layout.row#lp__banner-duplo-v3-mob",
            "flex-layout.row#seo__content"
        ]
    },

    // BANNER TOPO
    "flex-layout.row#lp__banner-topo": {
        "title": "Banner topo",
        "props": {
            "blockClass": "lp__banner-topo",
            "marginBottom": 3
            
        },
        "children": ["image#lp__banner-topo"]
    },

    "image#lp__banner-topo": {
        "props": {
            "src": "assets/images/LP/banner-topo-1.webp",
            "blockClass": "full"
        }
    },

